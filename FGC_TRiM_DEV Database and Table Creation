CREATE SCHEMA "FGC_TRIM_DEV";

CREATE TABLE "FGC_TRIM_DEV"."REF_STATION_CALLSIGNS"(
"STATION_ID" CHAR (36) NOT NULL,
"STATION_CALLSIGN_LOCAL" VARCHAR(12) NOT NULL,
"STATION_CALLSIGN_NATIONAL" VARCHAR(12) NOT NULL,
"STATION_NAME" VARCHAR(50) NOT NULL,
"STATION_COUNTY" VARCHAR(50) NOT NULL,
CONSTRAINT "PK" PRIMARY KEY ("STATION_ID")
);

CREATE TABLE "FGC_TRIM_DEV"."REF_APP_CALLSIGNS"(
"APPLIANCE_ID" CHAR (36) NOT NULL,
"APPLIANCE_CALLSIGN" VARCHAR (12) NOT NULL,
"APPLIANCE_STATUS" VARCHAR (100) NOT NULL,
CONSTRAINT "PK" PRIMARY KEY ("APPLIANCE_ID")
);

CREATE TABLE "FGC_TRIM_DEV"."REF_APP_STAT_GATEWAY"(
"APPLIANCE_ID" CHAR (36) NOT NULL,
"STATION_ID" CHAR (36) NOT NULL,
CONSTRAINT "FK_APP_ID" FOREIGN KEY ("APPLIANCE_ID")
REFERENCES "FGC_TRIM_DEV"."REF_APP_CALLSIGNS" ("APPLIANCE_ID")
ON DELETE RESTRICT,
CONSTRAINT "FK_STAT_ID" FOREIGN KEY ("STATION_ID")
REFERENCES "FGC_TRIM_DEV"."REF_STATION_CALLSIGNS" ("STATION_ID")
ON DELETE RESTRICT
);

CREATE TABLE "FGC_TRIM_DEV"."REF_WATCH_TYPES"(
"WATCH_ID" CHAR (36) NOT NULL,
"WATCH_LABEL" VARCHAR (50) NOT NULL,
CONSTRAINT "PK" PRIMARY KEY ("WATCH_ID")
);

CREATE TABLE "FGC_TRIM_DEV"."TRIM_REFERRAL"(
"REFERRAL_ID" CHAR (36) NOT NULL,
"SUBMITTED_BY" VARCHAR (12) NOT NULL,
"SUBMITTED_DATE" TIMESTAMP (0) NOT NULL DEFAULT CURRENT_TIMESTAMP,
"INCIDENT_NO" VARCHAR (12) NOT NULL,
"INCIDENT_DATE" TIMESTAMP (0) NOT NULL,
"INCIDENT_COMMANDER" VARCHAR (12) NOT NULL,
"DESCRIPTION" VARCHAR (1000) NOT NULL,
"INCIDENT_ADDRESS" VARCHAR (200) NOT NULL,
"STATION_ID" CHAR (36) NOT NULL,
"WATCH_ID" CHAR (36) NOT NULL,
"APPLIANCE_ID" CHAR (36) NOT NULL,
CONSTRAINT "PK" PRIMARY KEY ("REFERRAL_ID"),
CONSTRAINT "FK_STAT_ID" FOREIGN KEY ("STATION_ID")
REFERENCES "FGC_TRIM_DEV"."REF_STATION_CALLSIGNS" ("STATION_ID")
ON DELETE RESTRICT,
CONSTRAINT "FK_WATCH_ID" FOREIGN KEY ("WATCH_ID")
REFERENCES "FGC_TRIM_DEV"."REF_WATCH_TYPES" ("WATCH_ID")
ON DELETE RESTRICT,
CONSTRAINT "FK_APP_ID" FOREIGN KEY ("APPLIANCE_ID")
REFERENCES "FGC_TRIM_DEV"."REF_APP_CALLSIGNS" ("APPLIANCE_ID")
ON DELETE RESTRICT
);

CREATE TABLE "FGC_TRIM_DEV"."NOTES"(
"REFERRAL_ID" CHAR (36) NOT NULL,
"NOTES" VARCHAR (1000) NULL,
"PROCESS_STAGE_ID" CHAR (36) NOT NULL,
CONSTRAINT "FK_REFERRAL_ID" FOREIGN KEY ("REFERRAL_ID")
REFERENCES "FGC_TRIM_DEV"."TRIM_REFERRAL" ("REFERRAL_ID")
ON DELETE RESTRICT,
CONSTRAINT "FK_PROCESS_STAGE" FOREIGN KEY ("PROCESS_STAGE_ID")
REFERENCES "FGC_TRIM_DEV"."REF_PROCESS_STAGE" ("PROCESS_STAGE_ID")
ON DELETE RESTRICT
);

CREATE TABLE "FGC_TRIM_DEV"."INDIVIDUALS_INVOLVED"(
"REFERRAL_ID" CHAR (36) NOT NULL,
"INDIVIDUAL_EMPLOYEE_ID" VARCHAR (500) NOT NULL,
"INDIVIDUAL_ROLE_ID" CHAR (36) NOT NULL, 
"PROCESS_STAGE_ID" CHAR (36) NOT NULL,
CONSTRAINT "FK_REFERRAL_ID" FOREIGN KEY ("REFERRAL_ID")
REFERENCES "FGC_TRIM_DEV"."TRIM_REFERRAL" ("REFERRAL_ID")
ON DELETE RESTRICT,
CONSTRAINT "FK_PROCESS_STAGE" FOREIGN KEY ("PROCESS_STAGE_ID")
REFERENCES "FGC_TRIM_DEV"."REF_PROCESS_STAGE" ("PROCESS_STAGE_ID")
ON DELETE RESTRICT,
CONSTRAINT "FK_IND_ROLE_ID" FOREIGN KEY ("INDIVIDUAL_ROLE_ID")
REFERENCES "FGC_TRIM_DEV"."REF_INDIVIDUAL_ROLES" ("INDIVIDUAL_ROLE_ID")
ON DELETE RESTRICT
);

CREATE TABLE "FGC_TRIM_DEV"."REF_PROCESS_STAGE"(
"PROCESS_STAGE_ID" CHAR (36) NOT NULL,
"PROCESS_STAGE_LABEL" VARCHAR (200) NOT NULL,
"CREATED_BY" VARCHAR (12) NOT NULL,
"CREATED_TMS" TIMESTAMP(0) NOT NULL DEFAULT CURRENT TIMESTAMP,
"UPDATED_BY" VARCHAR (12) NULL DEFAULT NULL,
"UPDATED_TMS" TIMESTAMP(0) NULL DEFAULT NULL,
CONSTRAINT "PK" PRIMARY KEY ("PROCESS_STAGE_ID")
);

CREATE TABLE "FGC_TRIM_DEV"."REF_INDIVIDUAL_ROLES"(
"INDIVIDUAL_ROLE_ID" CHAR (36) NOT NULL,
"INDIVIDUAL_ROLE_LABEL" VARCHAR (200) NOT NULL,
"CREATED_BY" VARCHAR (12) NOT NULL,
"CREATED_TMS" TIMESTAMP(0) NOT NULL DEFAULT CURRENT TIMESTAMP,
"UPDATED_BY" VARCHAR (12) NULL DEFAULT NULL,
"UPDATED_TMS" TIMESTAMP(0) NULL DEFAULT NULL,
CONSTRAINT "PK" PRIMARY KEY ("INDIVIDUAL_ROLE_ID")
);

CREATE TABLE "FGC_TRIM_DEV".""

